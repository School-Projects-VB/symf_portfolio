<!DOCTYPE html>
<html lang="{% block lang %}{% endblock %}">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/style.css') }}">
    {% endblock %}

    {% block javascripts %}
        {# {{ encore_entry_script_tags('app') }} #}
    {% endblock %}
</head>
<body>
<ul id="navbar">
    <li><a href="{{ path('home') }}">{{ 'navbar.home'|trans }}</a></li>
    <li><a href="{{ path('projects') }}">{{ 'navbar.projects'|trans }}</a></li>
    <li><a href="{{ path('cv') }}">{{ 'navbar.cv'|trans }}</a></li>
    <li><a href="{{ path('contact') }}">{{ 'navbar.contact'|trans }}</a></li>
    <li><a href="{{ path('form_new') }}">Form</a></li>
</ul>
{% block content %}{% endblock %}
</body>
</html>
